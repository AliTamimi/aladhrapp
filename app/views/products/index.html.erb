<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Ownbyalahra</th>
      <th>Other</th>
      <th>Isdishdash</th>
      <th>Price</th>
      <th>Isactive</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.product %></td>
        <td><%= product.ownbyalahra %></td>
        <td><%= product.other %></td>
        <td><%= product.isDishdash %></td>
        <td><%= product.price %></td>
        <td><%= product.isActive %></td>
        <td><%= product.user %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
